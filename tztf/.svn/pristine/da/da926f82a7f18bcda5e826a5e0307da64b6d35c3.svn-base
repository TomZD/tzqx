/* 公共样式 */
body {
    font-family: "微软雅黑";
    font-size: 12px;
    color: #333333;
    background-color: #f0f1f2;
}

body, ul, li, p, form, dl, dt, dd, h1, h2, h3, h4, h5, h6, input, select, i {
    margin: 0;
    padding: 0;
}

    ul li {
        list-style: none;
    }

img {
    border: none;
}

a img {
    border: none;
}

a {
    color: #333333;
    text-decoration: none;
    outline: none;
}

    a:hover {
        text-decoration: none;
    }

    a:active {
        text-decoration: none;
    }

sub, sup {
    font-size: 75%;
    line-height: 0;
    position: relative;
    vertical-align: baseline;
}

sub {
    bottom: -0.25em;
}

sup {
    top: -0.5em;
}

em {
    font-style: normal;
}


.top-logo {
    position: fixed;
    top: 0px;
    left: 0px;
    z-index: 999;
    background: url("map/top-bg.png") repeat-x;
    height: 65px;
    width: 100%;
}

    .top-logo span {
        display: block;
        float: left;
        margin-top: 13px;
        margin-left: 15px;
    }

    .top-logo ul {
        float: right;
        overflow: hidden;
        padding-top: 9px;
        padding-right: 20px;
    }

        .top-logo ul li {
            float: left;
        }

            .top-logo ul li a {
                display: block;
                width: 100px;
                height: 44px;
                line-height: 40px;
                text-align: center;
                font-size: 16px;
                color: #fff;
                font-weight: bold;
                text-shadow: -1px -1px 0 #3e3e3e,1px 1px 0 #3e3e3e,1px 1px 0 #3e3e3e;
                -moz-text-shadow: -1px -1px 0 #3e3e3e,1px 1px 0 #3e3e3e,1px 1px 0 #3e3e3e;
                -webkit-text-shadow: -1px -1px 0 #3e3e3e,1px 1px 0 #3e3e3e,1px 1px 0 #3e3e3e;
                -o-text-shadow: -1px -1px 0 #3e3e3e,1px 1px 0 #3e3e3e,1px 1px 0 #3e3e3e;
            }

                .top-logo ul li a:hover {
                    background: url("map/menu-bg.png") no-repeat;
                }

                .top-logo ul li a.hov {
                    background: url("map/menu-bg.png") no-repeat;
                }

.exp {
    width: 260px;
    padding-bottom: 15px;
    border: solid 1px #9e9e9e;
    background: url("map/exp-bg.png") repeat;
    border-radius: 8px;
    -moz-border-radius: 8px;
    -webkit-border-radius: 8px;
    -o-border-radius: 8px;
    box-shadow: 5px 10px 10px rgba(0,0,0,.5);
    -moz-box-shadow: 5px 10px 10px rgba(0,0,0,.5);
    -webkit-box-shadow: 5px 10px 10px rgba(0,0,0,.5);
    -o-box-shadow: 5px 10px 10px rgba(0,0,0,.5);
}

.exp-p0 {
    position: absolute;
    top: 100px;
    right: 150px;
    z-index: 10;
    text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -moz-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -webkit-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -o-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
}


.exp-slider {
    position: absolute;
    top: 100px;
    right: 250px;
    z-index: 10;
    text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -moz-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -webkit-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -o-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
}

.exp-p1 {
    width: 412px;
    height: 55px;
    padding-top: 10px;
    padding-left: 5px;
    text-align: center;
    font-size: 14px;
    font-weight: bold;
    text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -moz-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -webkit-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    -o-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
}

    .exp-p1 span {
        font-size: 20px;
        font-weight: bold;
        padding-bottom: 5px;
        text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
        -moz-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
        -webkit-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
        -o-text-shadow: -1px -1px 0 #fff,1px 1px 0 #fff,1px 1px 0 #fff;
    }

    .exp-p1 em {
        display: block;
    }

.exp-p2 {
    margin-left: 18px;
    padding-top: 13px;
}

select.sel {
    margin: 0px;
    width: 155px;
    height: 29px;
    border: solid 1px #a9a9a9;
}

.exp-p3 {
    position: absolute;
    top: 80px;
    right: 10px;
    z-index: 10;
}

.exp-p33 {
    position: absolute;
    left: 10px;
    bottom: 10px;
    z-index: 10;
}

    .exp-p3 label, .exp-p33 label {
        float: left;
        display: block;
        /*width: 55px;*/
        padding: 0px 20px;
        height: 40px;
        line-height: 40px;
        text-align: center;
        background: #83a1c9;
        border: solid 0px #83a1c9;
        border-right: none;
        cursor: pointer;
        color: #fff;
        font-size: 14px;
    }

        .exp-p3 label:hover, .exp-p33 label:hover {
            background: #0d5ab1;
        }

        .exp-p3 label.label-hov, .exp-p33 label.label-hov {
            background: #0d5ab1;
            border: solid 0px #007eb8;
            /*font-weight: bold;*/
        }

        .exp-p3 label input, .exp-p33 label input {
            display: none;
        }

.exp-p4 {
    text-align: center;
    padding: 10px 0 10px 0;
    background: url("map/exp-bg1.png") repeat;
    border-bottom: solid 1px #bbbbbb;
}

    .exp-p4 select.sel {
