/*首页表格*/
.index-tbl {
    table-layout: fixed;
    width: 100%;
    margin-bottom: 0;
}

    .index-tbl td {
        padding: 0;
    }

    .index-tbl tbody td {
        *vertical-align: middle;
        background: #fff;
        border-width: 0px;
    }

        .index-tbl tbody td a.index-fund-name {
            color: #404040;
        }

    .index-tbl td .tbl-w {
        padding: 0 2px 0 2px;
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
    }

    .index-tbl thead td {
        height: 40px;
        line-height: 40px;
        background: #ebebeb;
        font-size: 14px;
        color: #626262;
    }

    .index-tbl tbody td {
        height: 32px;
        line-height: 32px;
        color: #343434;
        font-size: 14px;
        font-weight: normal;
    }

    .index-tbl thead td span {
        display: inline-block;
        vertical-align: middle;
    }

    .index-tbl td.last {
        text-align: left;
        padding-right: 15px;
    }

    .index-tbl thead td i.i-arw-d, .index-tbl thead td i.i-arw-t {
        vertical-align: middle;
        cursor: pointer;
        margin-left: 5px;
    }

    .index-tbl thead td i.i-var {
        vertical-align: middle;
        cursor: pointer;
        margin-left: 5px;
    }

.i-arw-d, .i-arw-t {
    display: inline-block;
    width: 0;
    height: 0;
    border-width: 6px;
    margin-bottom: -6px;
    border-style: solid dashed dashed;
    overflow: hidden;
    border-color: #acacac transparent transparent;
}

.i-arw-t {
    -moz-transform: scaleY(-1);
    -webkit-transform: scaleY(-1);
    -o-transform: scaleY(-1);
    transform: scaleY(-1);
    /*IE*/
    filter: FlipV;
    margin-top: -4px;
    margin-bottom: 0;
}

.i-var {
    display: inline-block;
    width: 10px;
    height: 12px;
    background: url(//qian-img.tenpay.com/web/v2/img/index/index_sp.png) no-repeat;
}

