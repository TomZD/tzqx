        }

        .aqi-level ul li .line {
            border-bottom: solid 1px #c9c9c9;
        }

.aqi-level .legend-text {
    float: left;
    line-height: 21px;
}

.aqi-level .legend-item {
    border: 1px solid #21bb0c;
    background: #46b035;
    float: left;
    margin-left: 1px;
    padding: 2px 12px;
    color: #fff;
    position: relative;
    overflow: hidden;
    text-decoration: none;
}

    .aqi-level .legend-item:hover {
        overflow: visible;
    }

    .aqi-level .legend-item .legend-item-wrapper {
        position: absolute;
        height: 45px;
        top: -46px;
        left: -34px;
    }

    .aqi-level .legend-item .legend-item-tip {
        position: relative;
        border: 1px solid #acb2bb;
        color: #333;
        white-space: nowrap;
        background-repeat: no-repeat;
        background-position: 14px 50%;
        padding: 6px 14px 6px 6px;
        height: 26px;
        width: 95px;
        line-height: 26px;
        text-align: center;
        box-sizing: content-box;
    }

.legend-item-tip {
    background: #fff;
}

.aqi-level .legend-item .legend-item-tip-arrow {
    
    width: 7px;
    height: 5px;
    position: absolute;
    left: 50%;
    margin-left: -3px;
    top: 38px;
}

.aqi-level .legend-1 {
    border-color: #38a800;
    background: #38a800;
}

.aqi-level .legend-2 {
    border-color: #8bd100;
    background: #8bd100;
}

.aqi-level .legend-3 {
    border-color: #ffff00;
    background: #ffff00;
    color: blue;
}

.aqi-level .legend-4 {
    border-color: #ff8000;
    background: #ff8000;
}

.aqi-level .legend-5 {
    border-color: #ff0000;
    background: #ff0000;
}

.aqi-level ul li i.aqi1-icon {
    background: #00ff00;
}

.aqi-level ul li i.aqi2-icon {
    background: #ffff00;
}

.aqi-level ul li i.aqi3-icon {
    background: #ff7e00;
}

.aqi-level ul li i.aqi4-icon {
    background: #ff0000;
}

.aqi-level ul li i.aqi5-icon {
    background: #99004c;
}

.aqi-level ul li i.aqi6-icon {
    background: #7e0023;
}

