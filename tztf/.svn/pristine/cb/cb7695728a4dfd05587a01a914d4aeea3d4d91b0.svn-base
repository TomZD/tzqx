}
.olLegend-min {
  min-width: 50px;
  text-align: center;
}
.olLegend-min .olLegend-title {
  padding-right: 5px;
}
.olLegend-min .olLegend-button,
.olLegend-min .olLegend-content {
  display: none;
}
