/* é£Žåœˆ */
.wind-ring {
  width: 50px;
  height: 50px;
  margin: 0 auto;
  background: url(../images/gigis/windring.png) no-repeat;
  background-size: 100% 100%;
}
.animation {
  animation: myfirst 2s infinite linear;
  -ms-animation: myfirst 2s infinite linear;
  -webkit-animation: myfirst 2s infinite linear;
  /* Safari and Chrome */
  -moz-animation: myfirst 2s infinite linear;
  /* Firefox */
  -o-animation: myfirst 2s infinite linear;
  /* Opera */
}
@keyframes myfirst {
  0% {
    transform: rotate(0deg);
  }
  50% {
    width: 48px;
    height: 45px;
  }
  100% {
    transform: rotate(360deg);
    width: 50px;
    height: 50px;
  }
}
@-ms-keyframes myfirst {
  0% {
    -ms-transform: rotate(0deg);
  }
  50% {
    width: 48px;
    height: 45px;
  }
  100% {
    -ms-transform: rotate(360deg);
    width: 50px;
    height: 50px;
  }
}
@-webkit-keyframes myfirst {
  0% {
    -webkit-transform: rotate(0deg);
  }
  50% {
    width: 48px;
    height: 45px;
  }
  100% {
    -webkit-transform: rotate(360deg);
    width: 50px;
    height: 50px;
  }
}
@-moz-keyframes myfirst {
  0% {
    -moz-transform: rotate(0deg);
  }
  50% {
    width: 48px;
    height: 45px;
  }
  100% {
    -moz-transform: rotate(360deg);
    width: 50px;
    height: 50px;
  }
}
@-o-keyframes myfirst {
  0% {
    -o-transform: rotate(0deg);
  }
  50% {
    width: 48px;
    height: 45px;
  }
  100% {
    -o-transform: rotate(360deg);
    width: 50px;
    height: 50px;
  }
}
