package cn.movinginfo.tztf.releasechannel;

/**
 * 
 * @ClassName: Weibo
 * @Description 微博传输对象
 * @author fl
 * @date 2016年8月18日 上午11:57:43
 */
public class Weibo {

	private String id;//站点ID,如：58457、58459
	private String content;//微博内容
	private String img;//图片通过base64转化的二进制的字符串
	private String fabuStatus;//发布状态
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getImg() {
		return img;
	}
	public void setImg(String img) {
		this.img = img;
	}
	public String getFabuStatus() {
		return fabuStatus;
	}
	public void setFabuStatus(String fabuStatus) {
		this.fabuStatus = fabuStatus;
	}
	
	
	
}
