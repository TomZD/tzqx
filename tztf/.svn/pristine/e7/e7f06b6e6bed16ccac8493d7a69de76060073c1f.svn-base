#allmap{
	height: calc(100% - 5rem);
	width: 100%
}
.confirm{
	width: 9.133333rem;
	height: 1.173333rem;
	background: #3399ff;
	border-radius: 10px;
	color: #fff;
	font-size: 0.533333rem;
	position: absolute;
	bottom: 0.4rem;
	left: 0.4rem;
}
.address{
	height:3rem;
	width:100%;
	background-color:#fff;
}
.address td{
	border:1px solid #ddd;
	text-align:center;
	font-size:0.373333rem;
}
.address td:first-child{
	width:40%;
}
.address td input{
	padding-left:0.2rem;
	width:100%;
	height:100%;
}