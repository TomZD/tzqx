/*公共*/
body {
	overflow-y: auto;
  overflow-x: hidden;
  margin: 0;
  padding: 0;
  min-width: 1366px;  
  
}
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td {
    margin: 0;
    padding: 0;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
    white-space: normal;
    line-height: normal;
    font-weight: normal;
    font-size: medium;
    font-variant: normal;
    font-style: normal;
    color: -webkit-text;
    text-align: start;
}
input,button,select,textarea{outline:none}
li {
  list-style-type:none;
}
ul{
	padding: 0;
}
body,span,a{
  font-family: 'Microsoft YaHei';
}
a{
  text-decoration: none;
}
a:hover{
  text-decoration: underline;
  color: #FFF;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
img{
    border: none;
}
body{
  background: url(../../../images/zt/tf/bg_img.png) no-repeat #00003a;
  background-size: contain;
  min-width: 1366px;
}
/*头部*/
.g-hd{
  margin: 0 auto;
  margin-top: 31.5%;
}
.m-carousel{
    background: rgba(255,255,255,0.15);
    width: 18%;
    height: 36px;
    border-radius: 25px;
    position: relative;
    margin: 0 auto;
    padding: 0 10px;
}
.carousel_con {
    display: inline-block;
    padding-right: 40px;
}
.carousel_btn {
    float: left;
    padding: 8px;
    width: 13%;
}
.carousel_txt {
    color: #FFF;
    line-height: 36px;
    font-size: 12px;
}
/*中间模块*/
.g-bd{
  width: 100%;
  margin: 0 auto;
  position: relative;
  top:15px;
  margin-bottom: 7%;
}
/*公共模块*/
.m-module {
  width: 100%;
  background: #181f47;
  -moz-box-shadow:1px 2px 2px #00000f;
  -webkit-box-shadow:1px 2px 2px #00000f;
  box-shadow:1px 2px 2px #00000f;
  margin-top: 10px;
}
.plr3{
  padding: 1%;
  width: 98%;
  position: relative;
}
.m-module-hd{
  padding: 1% 0;
  border-bottom: 1px solid #2d3566;
}
.m-module-hd img{
    position: relative;
    top: 6px;
    left: 33px;
}
.m-module-hd h3{
  color: #ffa409;
  display: inline-block;    
    text-indent: 1.5em;
    font-size: 30px;
}
.m-more{
  float: right;
  color: #6e71a6;
  position: relative;
  right: 4%;
  top: 17px;
}
/*台风实况*/

/*头部*/
.typhoonlive_hd{
  position: absolute;
  z-index: 99991;
  height: 75px;
  width: 100%;
  background-image: -webkit-radial-gradient(150px 150px at 500px 94px,hsla(211,44%,30%,1),hsla(226,40%,24%,1)); 
  background-image: radial-gradient(150px 150px at 500px 94px,hsla(211,44%,30%,1),hsla(226,40%,24%,1));
  -moz-box-shadow: 0px 1px 5px rgba(10, 17, 40, 0.59);
  -webkit-box-shadow: 0px 1px 5px rgba(10, 17, 40, 0.59);
  box-shadow: 0px 1px 5px rgba(10, 17, 40, 0.59);
}
.typhoonlive_hd_time,.m-report{  
    background: url(../../../images/zt/tf/line01_bg.png) no-repeat right;
    height: 60px;
    padding: 8px 20px;
    float: left;
}
.typhoonlive_hd_time{
    width: 22%;  
}
.typhoonlive_hd span{
  display: block;
  text-align: center;
  line-height: 30px;
  color: #FFF;
  font-size: 14px;

}
.play_btn,.stop_btn{
  display: inline-block;
}
.play_btn{
    position: relative;
    top: 3px;
    left: 30px;  
}
.play_btn i{
  background: #FFF;
  border-radius: 50%;
    width: 10px;
    height: 10px;
    margin: 5px;
    display: inline-block;
}
.typhoonlive,.map{
    width: 100%;
    height: 837px;
}
/*台风报告单按钮*/
.m-report{
  background: url(../../../images/zt/tf/line01_bg.png) no-repeat right;
    width: 15%;
}
.report_btn{
    display: block;
    width: 123px;
    height: 31px;
    line-height: 31px;
    background: #1c3958;
    border-radius: 25px;
    margin: 0 auto;
    color: #FFF;
    text-align: center;
    margin-top: 15px;
}
.report_btn:hover{
    text-decoration: none;
    background: #252c59;
}
.map{
    position: relative;
    display: block;
    float: left;
    overflow: hidden;
}
/*图标*/
.msgTyDiv {
    border: 1px solid #578cae;
    background-color: #7baedb;
    position: absolute;
    right: 245px;
    top: 114px;
    line-height: 23px;
    border-radius: 3px;
    opacity: .8;
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
    transition: all 0.3s;
    z-index: 999;
}
.msgTyDiv.hidebg {
    border: 0px;
    background-color: transparent;
}
/*图标展开的内容*/
.disTitle {
    color: #000;
    float: right;
    margin-right: 6px;
    font-size: 12px;
}
.disTitle:hover{
    color: #000;  
}
.disTitle .legendS {
    width: 28px;
    height: 57px;
    background: url(../../../images/zt/tf/legend.png) no-repeat center;
    display: block;
}
.msgTyphoon {
    background-color: #d9e7f4;
    margin: 5px;
    margin-top: 26px;
    color: #323232;
    text-align: left;
    overflow: hidden;
}
.msgTyphoon label {
    display: block;
    font-size: 10px;
    width: 160px;
    margin-left: 8px;
}
.msgTyphoon input[type="checkbox"] {
    margin-right: 5px;
}
.msgTyphoon span {
    width: 60px;
    height: 11px;
    display: inline-block;
    float: right;
    margin-right: 10px;
    border-bottom-width: 3px;
    border-bottom-style: dotted;
}
.msgTyphoon .BABJ {
    border-bottom-color: #dc6672;
}
.msgTyphoon .BEHZ {
    border-bottom-color: #5fd2cf;
}
.msgTyphoon .RJTD {
    border-bottom-color: #5c68f2;
}
.msgTyphoon .PGTW {
    border-bottom-color: #dccd70;
}
.msgTyphoon .RCTP {
    border-bottom-color: #5d6771;
}
.msgTyphoon i {
    width: 10px;
    height: 10px;
    display: inline-block;
    margin-right: 3px;
    border-radius: 5px;
}
.msgTyphoon .tyscycle1 {
    background-color: #00fee2;
}
.msgTyphoon .tyscycle2 {
    background-color: #fef301;
}
.msgTyphoon .tyscycle3 {
    background-color: #fe8e2c;
}
.msgTyphoon .tyscycle4 {
    background-color: #ff0204;
}
.msgTyphoon .tyscycle5 {
    background-color: #ae01d9;
}
.msgTyphoon .tyscycle6 {
    background-color: #ae00d8;
}

/*底部月份选择*/
.typhoonTimeLine {
    width: 100%;
    height: 116px;
    background: url(../../../images/zt/tf/lineBg.png) repeat #2c3461;
    position: relative;
    top: 711px;
    z-index: 9999;
}
.searchCondition {
    background: #386dce;
    border-radius: 5px;
    height: 30px;
    position: absolute;
    top: -30px;
    left: 30px;
    font-family: "Microsoft YaHei","SimHei","san-serif";
}
.searchCondition .bindYear {
    display: block;
    float: left;
    color: #fff;
    font-size: 16px;
    line-height: 30px;
    margin: 0px 10px;
}
.yearOption {
    position: absolute;
    top: 27px;
    font-size: 12px;
    background: #000;
    width: 80px;
    max-height: 120px;
    overflow-y: auto;
    text-align: center;
    z-index: 999;
    line-height: 20px;
    filter: alpha(opacity=90);
    -moz-opacity: 0.9;
    -khtml-opacity: 0.9;
    opacity: 0.9;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    display: none;
}
.yearOption li {
    color: #fff;
    padding: 0px 10px;
}
input[placeholder], [placeholder], *[placeholder] {
    color: #668fd9;
}
.searchCondition .TYname {
    display: block;
    float: left;
    width: 200px;
    height: 24px;
    line-height: 30px;
    font-size: 12px;
    color: #b9d1e8;
    border: none;
    background: transparent;
}
.searchCondition .searchBtn {
    display: block;
    float: left;
    width: 30px;
    height: 30px;
    background: url(../../../images/zt/tf/searchBtn.png) no-repeat center;
    border: 0px;
    cursor: pointer;
}

.searchCondition .dropDetail {
    position: absolute;
    width: 100%;
    top: 26px;
    left: 0px;
    display: none;
    background: #698ed0;
    border: 1px solid #265aba;
    max-height: 120px;
    overflow-y: auto;
    z-index: 9;
}
.searchCondition .dropDetail li {
    color: #fff;
    line-height: 20px;
    height: 20px;
    border-bottom: 1px solid #8db5ff;
    font-style: normal;
    text-indent: 1em;
}
.TyShowBlock {
    position: absolute;
    width: 90%;
    height: 100%;
    margin: 0px auto;
    overflow: hidden;
}
.timelineBg {
    height: 100%;
}
.TyShowBlock .typhoonBox {
    position: absolute;
    padding: 0px 5px;
    color: #fff;
    font-size: 12px;
    cursor: pointer;
    filter: alpha(opacity=60);
    -moz-opacity: 0.6;
    -khtml-opacity: 0.6;
    opacity: 0.6;
    white-space: nowrap;
    text-align: center;
}
.TyShowBlock .typhoonBox:hover{
    filter:alpha(opacity=100); /*IE滤镜，透明度50%*/
-moz-opacity:1.0; /*Firefox私有，透明度50%*/
 -khtml-opacity:1.0;/* 老版Safari */
opacity:1.0;/*其他，透明度50%*/
}
.timeLine {
    height: 16px;
    line-height: 14px;
    color: #fff;
    position: absolute;
    top: 100px;
}
.timeLine .myscale {
    display: block;
    float: left;
    text-align: center;
}
.timeLine .tag {
    display: block;
    width: 3px;
    height: 14px;
    float: right;
    background: #fff;
}
.ajaxnotice {
    font-size: 16px;
    color: #fff;
    position: absolute;
    top: 40px;
    left: 50%;
}
.warnTips {
    position: absolute;
    width: 160px;
    padding: 20px;
    display: none;
    background: #b9d1e8;
    border: 1px solid #386dce;
    border-radius: 5px;
    z-index: 999;
    top: 30px;
}
.warnTips .warnTxt {
    white-space: nowrap;
}
.typhoonTimeLine input.Arrow {
    width: 24px;
    height: 19px;
    position: absolute;
    top: 46px;
    background: url(../../../images/zt/tf/arrow.png) no-repeat;
    cursor: pointer;
    border: none;
}
.typhoonTimeLine input#arrowLastYear {
    background-position: 0px 0px;
    left: 26px;
}
.typhoonTimeLine input#arrowNextYear {
    background-position: -24px 0px;
    right: 20px;
}
.amountnotice {
    position: absolute;
    top: -40px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    width: 0px;
    right: 0px;
    background: rgba(0,0,0,0.5);
    color: #fff;
    border-top-left-radius: 5px;
    display: none;
}
/*右边展示*/
.g-right {
    position: absolute;
    z-index: 9999;
    right: 0px;
    -moz-box-shadow: 0px 2px 6px #000;
    -webkit-box-shadow: 0px 2px 6px #000;
    box-shadow: 0px 2px 6px #000;
    background: #181f47;
    height: 827px;
    margin-top: -117px;
}
.on_right_btn {
    position: absolute;
    z-index: 100;
    top: 0;
    right: 0;
    background: url(../../../images/zt/tf/on_right_btn.png);
    width: 25px;
    height: 17px;
    display: block;
    top: -40px;
}
.suspension{
    position: relative;
    /* 
    top: 70px;
     */
}
.g-right .suspension_times {
    width: 213px;
    height: auto;    
    padding: 4px 10px;
    background: #252c59;
    box-shadow: 2px 0px 2px rgba(0,0,0,0.2);
    border-top: 5px solid #11132c;
}
.windBox {
    border-radius: 3px;
    background: #3b4477;
    box-shadow: inset 0 0 1px rgba(255,255,255,0.3);
    padding: 10px 6px;
    display: inline-block;
    margin: 5px 5px 10px 0px;
    position: relative;
    cursor: pointer;
}
.windBox.current {
    border: 1px solid #45b1f9;
    box-shadow: 5px 5px 5px rgba(0,0,0,0.75);
}
.windBox span {
    display: block;
    font-family: "Microsoft YaHei","SimHei","san-serif";
}
.windBox span.windName {
    font-size: 16px;
    color: #fff;
}
.windBox span.formateTime {
    font-size: 14px;
    color: #7d87c1;
}
.windBox i.closeBtn {
    display: block;
    width: 14px;
    height: 14px;
    background: url(../../../images/zt/tf/closeBtn.png) no-repeat center;
    position: absolute;
    top: -5px;
    right: -5px;
    cursor: pointer;
}
.windBox i.closeBtn:hover{
    filter:alpha(opacity=70); /*IE滤镜，透明度50%*/
-moz-opacity:0.7; /*Firefox私有，透明度50%*/
 -khtml-opacity:0.7;/* 老版Safari */
opacity:0.7;/*其他，透明度50%*/
}
.m-ranking {
    background: #181f47;
    border: 1px solid #181a3a;
    width: 211px;
    padding: 4px 10px;
    height: auto;
    line-height: 25px;
}
.m-ranking .mTit .titIcon {
    display: block;
    width: 31px;
    height: 26px;
    float: left;
    background: url(../../../images/zt/tf/typhoon.png) no-repeat;
    margin: 5px 0px;
}
.m-ranking .mTit .curTyname {
    color: #c5e9ff;
    line-height: 40px;
    margin-left: 10px;
    font-size: 16px;
    font-family: "Microsoft YaHei","SimHei","san-serif";
}
.windTab {
    width: 100%;
    font-family: "Microsoft YaHei","SimHei","san-serif";
    color: #fff;
    position: relative;
}
.windTab tr th {
    position: relative;
}
.windTab i.titBg {
    display: block;
    width: 35px;
    height: 35px;
    line-height: 35px;
    background: url(../../../images/zt/tf/shadowBg.png) no-repeat center;
    margin: 5px;
    font-style: normal;
    font-weight: normal;
    font-size: 12px;
    text-align: center;
}
.windTab tr th span.tag {
    position: absolute;
    top: 42px;
    left: 9px;
    font-size: 12px;
    font-weight: normal;
    color: #738098;
}
.detailInner{
   margin-top: 28px;
   overflow-y: auto; 
   padding: 0px; 
   width: 211px; 
   outline: none; 
}
.detailInner table {
    width: 100%;
}
.windTab tbody tr {
    cursor: pointer;
}
.windTab tr td {
    text-align: center;
    height: 24px;
    line-height: 24px;
    font-size: 12px;
    position: relative;
}

.windTab span.timeBind {
    display: block;
    color: #ec6a86;
    text-align: left;
    text-indent: 10px;
}
.windTab tr td em.trBg {
    width: 0px;
    height: 21px;
    position: absolute;
    left: 8px;
    background: url(../../../images/zt/tf/filterBg.png) no-repeat;
    margin-top: 3px;
}
.windTab span.bg01 {
    display: block;
    width: 36px;
    margin: 0px auto;
    background: rgba(121,128,183,0.1);
    color: #6e71a6;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#197980b7,endColorstr=#197980b7);
}
.windTab span.bg02 {
    display: block;
    width: 36px;
    margin: 0px auto;
    background: rgba(255,255,255,0.1);
    color: #c2c4ef;
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#19ffffff,endColorstr=#19ffffff);
}
/*选中浮层的关闭按钮样式*/
.popdivclose{
    position:absolute;
    width:12px;
    height:12px;
    top:3px;
    right:3px;
    background:url(/hztq/r/cms/static/js/lib/gigis/images/close.png) no-repeat center;
}
.popdivclose:hover{
    filter:alpha(opacity=70); /*IE滤镜，透明度50%*/
-moz-opacity:0.7; /*Firefox私有，透明度50%*/
 -khtml-opacity:0.7;/* 老版Safari */
opacity:0.7;/*其他，透明度50%*/
}
