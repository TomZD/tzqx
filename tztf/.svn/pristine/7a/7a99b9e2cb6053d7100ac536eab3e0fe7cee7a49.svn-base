  text-align: center;
  margin-top: 23px;
}
.m-knowledge ul li{
  display: inline-block;
  text-align: center;
}
.m-knowledge ul li a img{
  width: 90%;
}
.m-knowledge ul li a p{
    background: rgba(0,0,0,0.4);
    width: 91%;
    line-height: 27px;
    text-align: center;
    color: #FFF;
    position: relative;
    top: -47px;    
    left: 5%;
}
/*滚动条 start*/  
::-webkit-scrollbar {  
  width: 10px;  
  height: 10px; 
  background-color: #181f47;  
}  
/*定义滚动条轨道 内阴影+圆角*/  
::-webkit-scrollbar-track { 
  background: #181f47 ;  
} 
/*定义滑块 内阴影+圆角*/  
::-webkit-scrollbar-thumb {   
  width:6px;
  height:6px;
  border-radius: 3px;    
  background-color:#737bac;  
}  
::-webkit-scrollbar-thumb:hover {  
  border-radius: 3px;  
  background-color:#919bd9;  
} 