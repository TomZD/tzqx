.g-aside {
  /* ico */
}
.g-aside a {
  display: block;
  text-align: center;
  color: white;
  padding-top: 20px;
  padding-bottom: 20px;
  cursor: pointer;
}
.g-aside a span {
  display: block;
  line-height: 20px;
  height: 20px;
  overflow: hidden;
}
.g-aside a:hover {
  background: #f14459;
  text-decoration: none;
}
.g-aside a.on {
  background: #b82424;
}
.g-aside .ico-40-1 {
  background: url(/static/components/aside/images/a1.png) center center no-repeat;
}
