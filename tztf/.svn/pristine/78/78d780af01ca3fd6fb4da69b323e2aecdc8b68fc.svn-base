/*头部*/
.top-nav{
	position: relative;
	z-index: 1;
	height: 1.173333rem;
	width: 100%;
	box-sizing: border-box;
	background-color: #fff;
	display:none;
}
.top-nav i{
	left: 0.4rem;
	height: 0.466667rem;
	width: 0.266667rem;
	background: url(../images/back-blue.png);
	background-size: 100% 100%;
}
.top-back{
	left: 0.933333rem;
	color: #41a8ff;
	font-size: 0.426667rem;
	line-height: 1.173333rem;
}
.top-title{
	font-size: 0.48rem;
	color: #000;
	line-height: 1.173333rem;
}
.top-more{
	font-size: 0.426667rem;
	color: #41a8ff;
	right: 0.4rem;
	line-height: 1.173333rem;
}

.group-bg{
	width: 100%;
	height: 3.4rem;
	background-image: url(../images/group-info-bg.png);
	background-size: 100% 100%;
}
.name-info{
	width: 100%;
	height: 2.666667rem;
	position: absolute;
	padding: 0 0.4rem;
	box-sizing: border-box;
	transform: translateY(-50%);
}
.name-info ul{
	width: 65%;
    float: right;
    position: relative;
    top: 1.066667rem;
}
.name-info ul li{
	position: relative;
	height: 0.666667rem;
	display: block;
	width: 100%;
}
.name-info ul li:first-child span{
	font-size: 0.426667rem;
	float: left;
	line-height: 0.666667rem;
	color: #333;
}
.name-info ul li:last-child span{
	font-size: 0.32rem;
	float: left;
	line-height: 0.666667rem;
	color: #41a8ff;
}
.name-info ul li:first-child i{
	display: inline-block;
	right: 0;
	width: 0.466667rem;
	height: 0.466667rem;
	background-image: url(../images/edit.png);
	background-size: 100% 100%;
}
.group-bottom i{
	display: inline-block;
	right: 0.4rem;
	width: 0.466667rem;
	height: 0.466667rem;
	background-image: url(../images/edit.png);
	background-size: 100% 100%;
}
.name-info ul li:last-child i{
	display: inline-block;
	right: 0;
	width: 0.233333rem;
	height: 0.466667rem;
	background-image: url(../images/more.png);
	background-size: 100% 100%;
}
.name-bg{
	position: absolute;
	width: 100%;
	height: 2.666667rem;
	background-color: #fff;
	z-index: -1;
	transform: translateY(-50%);
}
.touxiang{
	display: inline-block;
	float: left;
	height: 2.4rem;
	width: 2.4rem;
	margin-top: 0.133333rem;
	margin-left: 0.533333rem;
	background-image: url(../images/touxiang.png);
	background-size: 100% 100%;
}
.peo-list{
	width: 100%;
	padding: 0.266667rem 0.4rem;
	box-sizing: border-box;
	background-color: #fff;
	margin-top: 1.6rem;
	clear: both;
}
.peo-list li{
	display: inline-block;
	height: 2.0rem;
	width:19%;
	position: relative;
}
.peo-list li i{
	top: 0.066667rem;
	height: 1.6rem;
	width: 1.6rem;
	background-image: url(../images/touxiang.png);
	background-size: 100% 100%;
}
.peo-list li:last-child i{
	background-image: url(../images/add.png);
	background-size: 100% 100%;
}
.peo-list li span{
	display: block;
	height: 0.466667rem;
	width: 95%;
	color: #666666;
	font-size: 0.32rem;
	text-align: center;
	line-height: 0.466667rem;
	background-color: #fff;
	border-radius: 5px;
	box-shadow: 0 0 0.213333rem 0.026667rem rgba(0,0,0,0.18);
	bottom: 0;
}
.group-bottom{
	position: relative;
	background-color: #fff;
	margin-top: 0.266667rem;
	padding: 0 0.4rem;
	box-sizing: border-box;
}
.group-bottom span:first-child{
	font-size: 0.426667rem;
	color: #333;
	line-height: 1.133333rem;
}
.group-bottom span:nth-child(2){
	margin-left: 0.533333rem;
	font-size: 0.4rem;
	color: #666;
	line-height: 1.133333rem;
}
.bulid{
	position: absolute;
	bottom: 0.933333rem;
	color: #fff;
    width: calc(100% - 0.8rem);
    margin:0 0.4rem;
    height: 1.066667rem;
    background: #3399ff;
    border-radius: 5px;
    font-size: 0.533333rem;
}
.cginfo.editing input{
	color:#ccc!important;
}
.cginfo.editing i{
	background-image:url(../images/edit-blue.png)!important;
}
.name-info ul li:first-child input {
    font-size: 0.426667rem;
    float: left;
    line-height: 0.666667rem;
    color: #333;
}
.group-bottom input {
    margin-left: 0.533333rem;
    font-size: 0.4rem;
    color: #666;
    line-height: 1.133333rem;
}