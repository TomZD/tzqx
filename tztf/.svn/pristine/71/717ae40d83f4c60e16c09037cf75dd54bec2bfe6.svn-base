@charset "utf-8";
body,h1,h2,h3,h4,h5,h6,hr,p,blockquote,dl,dt,dd,ul,ol,li,pre,form,fieldset,legend,button,input,textarea,th,td {
	margin: 0;
	padding: 0;
}
a,h1,h2,h3,p{
    font-family: 'Microsoft YaHei';
}
ol,ul,li {
	list-style: none;
}

img {
	border: 0;
}
img[src=""],img:not([src]){
      opacity:0;
 }
table {
	border-collapse: collapse;
	border-spacing: 0;
}

p {
	word-wrap: break-word;
}
a{
    text-decoration: none;
}
input, textarea, select,button {
    outline: none;
    resize: none;
	cursor: pointer;
    border:none;
    background: transparent;
}
    input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {
        -webkit-box-shadow: 0 0 0 1000px transparent inset !important;
        box-shadow: 0 0 0 1000px transparent inset !important;
        background-color:transparent !important;
        background-image: none; 
        outline:none;
    }
input, button {
    overflow: visible;
    font-family: inherit;
}
.clearfix {
	*zoom: 1;
}

.clearfix:before,.clearfix:after {
	display: table;
	content: "";
	line-height: 0;
}

.clearfix:after {
	clear: both;
}
.fl{
    float:left;
}
.fr{
    float:right;
}
.dpi{
    display: inline-block;
}
.dpb{
    display: block;
}
input[type="text"],textarea,body{
    font-family:"SimSun";
    font-size: 14px;
    color: #bebed8;
}
body {
	min-width: 1280px;
    overflow:hidden;
    min-height:672px;
    background: url(../images/bg_img.png) no-repeat;
    filter:"progid:DXImageTransform.Microsoft.AlphaImageLoader(sizingMethod='scale')";  
    -moz-background-size:100% 100%;  
    background-size:100% 100%;  
    behavior: url(../static/js/PIE/backgroundsize.min.htc);
}

/*head*/
.g-hd,.g-bottom{
    height: 57px;
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#33000000,endColorstr=#33000000);
    background: rgba(0, 0, 0, 0.21); 
    width: 100%;  
}

.g-bottom{
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#7F000000,endColorstr=#7F000000);
    background: rgba(0, 0, 0, 0.5); 
}
.g-hd .dpi img{
    margin: 11px 8px 0 16px;
}
.g-hd .dpi span{
    color: #FFF;
    font-size: 20px;  
    position: relative;
    top: -8px;
    font-family: "SimHei";
}
.g-hd .m-nav li{
    width: auto;
    height: 56px;
    text-align: center;
    line-height: 73px;
    display: inline-block;
    padding-right: 20px;
}
.g-hd .m-nav li a{
    display: inline-block;
    height: 56px;
    line-height: 56px;

}
.g-hd .m-nav li a i{
    background: url(../images/nav_btn.png) no-repeat;
    width: 31px;
    height: 22px;
    display: inline-block;
    position: relative;
    top: 6px;
    right: 12px;
}
/*图标*/
.g-hd .m-nav li a i.publishApplication_btn{
    background-position: -4px -3px;
}
.g-hd .m-nav li a i.relieveApplication_btn{
    background-position: 1px -40px;
}
.g-hd .m-nav li a i.historyQuery_btn{
    background-position: 1px -79px;
}
.g-hd .m-nav li a i.management_btn{
    background-position: 1px -118px;
}
.g-hd .m-nav li a i.contacts_btn{
    background-position: 1px -157px;
}
.g-hd .m-nav li a i.home_btn{
    background-position: -1px -191px;
}
.g-hd .m-nav li a i.unitManagement_btn{
    background-position: -1px -234px;
}
.g-hd .m-nav li a i.categoryManagement_btn{
    background-position: -1px -275px;
}
.g-hd .m-nav li a i.notice_btn{
    background-position: -4px -352px;
}
.g-hd .m-nav li a i.has_notice_btn{
	 background-position: -4px -313px;
}
.g-hd .m-nav li a i.yjya_btn{
	 background-position: -4px -387px;
}
/*图标悬浮和选中*/
.g-hd .m-nav li:hover a i.publishApplication_btn,.g-hd .m-nav li.on_nav a i.publishApplication_btn{
    background-position: -46px -3px;
}
.g-hd .m-nav li:hover a i.relieveApplication_btn,.g-hd .m-nav li.on_nav a i.relieveApplication_btn{
    background-position: -41px -40px;
}
.g-hd .m-nav li:hover a i.historyQuery_btn,.g-hd .m-nav li.on_nav a i.historyQuery_btn{
    background-position: -41px -79px;
}
.g-hd .m-nav li:hover a i.management_btn,.g-hd .m-nav li.on_nav a i.management_btn{
    background-position: -41px -118px;
}
.g-hd .m-nav li:hover a i.contacts_btn,.g-hd .m-nav li.on_nav a i.contacts_btn{
    background-position: -42px -157px;
}
.g-hd .m-nav li:hover a i.home_btn,.g-hd .m-nav li.on_nav a i.home_btn{
    background-position: -44px -191px;
}
.g-hd .m-nav li:hover a i.unitManagement_btn,.g-hd .m-nav li.on_nav a i.unitManagement_btn{
    background-position: -44px -234px;
}

.g-hd .m-nav li:hover a i.categoryManagement_btn,.g-hd .m-nav li.on_nav a i.categoryManagement_btn{
    background-position: -44px -275px;
}
.g-hd .m-nav li:hover a i.notice_btn,.g-hd .m-nav li.on_nav a i.notice_btn{
    background-position: -44px -352px;
}
.g-hd .m-nav li:hover a i.has_notice_btn,.g-hd .m-nav li.on_nav a i.has_notice_btn{
    background-position: -44px -313px;
}
.g-hd .m-nav li:hover a i.has_notice_btn,.g-hd .m-nav li.on_nav a i.yjya_btn{
    background-position: -44px -387px;
}
/*名称*/
.g-hd .m-nav li a span{
    color: #9d9dd3;
}
.g-hd .m-nav li:hover a span,.g-hd .m-nav li.on_nav a span{
    color: #7099ff;
}
/*用户*/
.g-hd .m-nav li.user img{
    border-radius: 50%;
    behavior: url(../static/js/PIE/PIE.htc);  
    position: relative;
    top: 4px;
    width: 34px;
    height: 34px;
    right: 10px;
}
.g-hd .m-nav li.user p{
    display: inline-block;
    position: relative;
    top: 3px;
}
.g-hd .m-nav li.user span{
    display: block;
    line-height: 24px;
}
/*iframe*/
.g-bd{
    padding: 28px 40px 37px 40px;
}
/*内容*/
/*左右宽度一样时*/
.g-bd-left,.g-bd-right-modular,.g-bd-cont{
    width: 49.1%;
    float: left;
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#33000000,endColorstr=#33000000);
    background: rgba(0, 0, 0, 0.21);
    border-radius: 3px;
    behavior: url(../static/js/PIE/PIE.htc);  
}
.g-bd-cont{
    width: 100%;
}
.g-bd-right-modular{
    width: 100%;
}
.g-bd-right{
    width: 49%;
    float: right;
    overflow-y: auto;
}
/*内容-标题*/
.g-bd-tit{
    height: 34px;
    line-height: 34px;
    border-bottom: 1px solid #415580;
    padding: 0 10px;
}
.g-bd-tit span{
    color: #ededf5;
}
/*输入框*/
.m-text-input,.m-text-textarea{
    border:1px solid #415580;
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#19000000,endColorstr=#19000000);
    background: rgba(0, 0, 0, 0.1);
    line-height: 34px;
    padding: 0 15px;
    border-radius: 4px;
    behavior: url(../static/js/PIE/PIE.htc);
}
/*底部*/
.g-bottom{
    height: 35px;
    line-height: 35px;
    position: absolute;
    bottom: 0px;
}
.g-bottom p{
    float: right;
    color: #FFF;
    margin-right: 52px;
}
/*按钮*/
.m-event-button{
    width: 60px;
    height: 25px;
    line-height: 25px;
    text-align: center;
    font-size: 12px;
    color: #FFF;
    background: #0f8ffe;
    position: relative;
    float: right;
    right: 12px;
    margin-top: -34px;
    border-radius: 3px;
    behavior: url(../static/js/PIE/PIE.htc);  
}
.m-event-button:hover{
    background: #56b0fe;
}
/*滚动条*/
::-webkit-scrollbar-thumb{  
   background-color:#59617c;  
   height:0px;  
   outline-offset:0px ;
   outline:0px solid ;  
   -webkit-border-radius:8px;  
 
}  
::-webkit-scrollbar-thumb:hover{  
   background-color:#707ca5;  
   height:0px;  
   -webkit-border-radius:8px;  
}   
::-webkit-scrollbar{  
   width:8px;  
   height:6px;  
   -webkit-border-radius:8px; 
}  
::-webkit-scrollbar-track-piece{  
   background-color:none; 
   -webkit-border-radius:0px; 
   box-shadow: inset 0 0 20px rgba(0, 0, 0, 0.1), 0 0 0px #6FF; 
}

/*工具栏*/
.m-tool{
    padding: 0 12px;
}
.m-tool ul li,.m-tool ul li select{
    display: inline-block;
    color: #bebed8;
    line-height: 60px;
}
.m-tool ul li select{
    width: 135px;
    border: 1px solid #415580;
    border-radius: 5px;
    height: 32px;
    background: rgba(0,0,0,0.1);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#19000000,endColorstr=#19000000);
    behavior: url(../static/js/PIE/PIE.htc);  
}
.m-table{
    width: 100%;
}
/*表格*/
.m-table,.m-table table{
    width: 100%;
}
.m-table table thead tr td{
    color:#fff;
}
.m-table table tr td{
    border-top: 1px solid #415580;
    border-bottom: 1px solid #415580;
    text-align: center;
    line-height: 40px;
    height: 40px;
    color:#bebed8;
}
/*表格中的偶数行*/
.m-table table tr:nth-child(even){
    background: rgba(0,0,0,0.1);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#19000000,endColorstr=#19000000);
}
/*页面*/
.m-page{
    float: right;
    margin-right: 15px;
    position: relative;
    top:12px;
    height: 64px;
}
.m-page div{
    padding:5px 15px;
    background: rgba(0,0,0,0.1);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#19000000,endColorstr=#19000000);
    border: 1px solid #415580;
    height: 24px;
    line-height: 24px;
    font-size: 12px;
    text-align: center;
    color: #626262;
    float: left;
    cursor: pointer;
    margin-left: -1px;
}
.m-page .home_page,.m-page .shadowe{
    height: 0px;
    width: 0px;
    border-left: 0px;
    border-top: 5px solid transparent;
    border-bottom: 5px solid transparent;
    padding:0px;
    margin-top: 7px;    
}
.m-page .home_page{
    border-right: 10px solid #ced0da;
    border-top-left-radius: 3px;
    border-bottom-left-radius: 3px;
}
.m-page .shadowe{
    border-left: 10px solid #ced0da;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}
.m-table tbody button.btn-delete{
    color:red;
}
tbody tr td input{
    border:1px solid #415580;
    background: rgba(0,0,0,0.1);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#19000000,endColorstr=#19000000);
    border-radius: 3px;
    text-align: center;
    height: 24px;
    width: 160px;
    behavior: url(../static/js/PIE/PIE.htc);  
}
tbody tr td input:focus{
    border: 1px solid #5695db;
    box-shadow: 0 0 5px rgba(86, 149, 219, 0.66);
    behavior: url(../static/js/PIE/PIE.htc);  
}
/*弹窗*/
.m-popup{
    width: 400px;
    height: auto;
    min-height: 235px;
    background: #fcfcfc;
    -webkit-box-shadow: 0px 0px 6px rgba(4, 0, 0, 0.21);
    -moz-box-shadow: 0px 0px 6px rgba(4, 0, 0, 0.21);
    box-shadow: 0px 0px 6px rgba(4, 0, 0, 0.21);
    border-radius: 2px;
    behavior: url(../static/js/PIE/PIE.htc);  
    position: fixed;
    top: 0;
    left: 40%;
    z-index: 99;
    display: none;
}
/*头部*/
.m-popup .popup_hd{
    height: 35px;
    line-height: 35px;
    background: #363e5a;
    
   
}
.m-popup .popup_hd span{
    color: #fff;
    font-size: 12px;
    text-indent: 1.5em;
    display: inline-block;
    font-weight: bold;
}
.m-popup .popup_hd .close_btn{
    background: url(../images/popup_close.png) no-repeat;
    background-position: 0 0;
    display: inline-block;
    width: 12px;
    height:12px;
    float: right;
    position: relative;
    top: 10px;
    right: 10px;
}
.m-popup .popup_hd .close_btn:hover{
    background-position: 0 -16px;
}
/*内容*/
.m-popup .popup_bd .status_cont{
    height: 26px;
    line-height: 26px;
    text-align: center;
}
.m-popup .popup_bd{
    text-align: center;
    color:#bebed8;
    font-size: 12px;  
}
/*单、复选按钮样式*/
.checkbox_bd,.m-checkbox {
    display: inline-block;
}
.checkbox_bd {
    width:85%;
}
.m-checkbox span {
    text-align: left;
    padding-right: 0px;
}
.checkbox, .radio {
    width: 16px;
    height: 16px;
    float: left;
    cursor: pointer;
    margin-top: 10px;
    text-align: center;
    background-image: url(../images/oncheck_img.png);
    background-repeat: no-repeat;
    background-position: -1px -2px;
}
.radio {
    background-position: -1px -24px;
    margin-left: -7px;
} 
.on_check {
    background-position: -21px -2px;
}
.on_input_check {
    background-position: -21px -24px;
}

.radioclass {
    opacity: 0;
    cursor: pointer;
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    filter: alpha(opacity=0);
    width: 0;
    height: 0;
}
/*下拉框*/
.m-select {
    position: relative;
}
.dropdown-menu {
    width: 99%;
    top: 18px;
    position: absolute;
    top: 29px;
    left: 0;
    z-index: 1000;
    float: left;
    padding: 5px 0;
    margin: 8px 0 0;
    text-align: center;
    border: 1px solid #415580;
    background: rgba(0, 0, 0, 0.8);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#C8000000,endColorstr=#C8000000);
    border-radius: 4px;
    -webkit-background-clip: padding-box;
    background-clip: padding-box;
    display: none;
}

.btn, .m-select {
    vertical-align: middle;
    display: inline-block;
    float: right;
}

    .m-select > .btn:first-child:not(:last-child):not(.dropdown-toggle), .lable-title {
        height: 36px;
    }

.g-bd-left-con ul li span.lable-title {
    line-height: 36px;
    float: left;
    text-align: left;
    width: auto;
}
.ico-up, .btn-link em {
    width: 0;
    height: 0;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    position: relative;
    float: right;
    cursor:pointer;
}
.m-sets .ico-up {
    top: 14px;
    border-top: 6px solid #FFF;
}
.m-sets .btn-link em {
    border-top: 6px solid #233c51;
    top: 12px;
    left: -12px;
    z-index: 999;
}
.m-sets-sets .ico-up {
    top: 10px;
    right: -10px;
    border-top: 6px solid #FFF;
}
.m-sets-sets .btn-link em {
    border-top: 6px solid #233c51;
    top: 14px;
    right: -10px;
    z-index: 999;
}
.dropdown-menu .label {
    display: block;
    line-height: 22px;
    overflow: hidden;
    color: #FFF;
    text-decoration: none;
    cursor: pointer;
    white-space: nowrap;
    font-size: 14px;
    width:99%;
}
    .dropdown-menu .label:hover {
        color: #59617c;
    }
/*放弃，取消等按钮*/

button.abandon{
    background: rgba(99,99,99,0.21);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#33636363,endColorstr=#33636363); 
    color: #9d9dd3;
}
button.abandon:hover{
    background: rgba(154,154,154,0.21);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#339A9A9A,endColorstr=#339A9A9A);  
}
/*上传按钮*/
button.report{
    background:-webkit-linear-gradient(left, #4a85f2, #7867f9);
    background:-o-linear-gradient(left, #4a85f2, #7867f9);
    background:linear-gradient(to right, #4a85f2, #7867f9);
    color: #FFF;
}   
button.report:hover{
    background:-webkit-linear-gradient(left, #6798f3, #9283fe);
    background:-o-linear-gradient(left, #6798f3, #9283fe);
    background:linear-gradient(to right, #6798f3, #9283fe);
}
