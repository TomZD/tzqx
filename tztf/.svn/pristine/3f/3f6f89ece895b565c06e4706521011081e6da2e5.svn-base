body{
    min-height: 720px;
}
.g-bd{
    padding: 40px 20px;
    background: url("../images/page_bk.png") 0 0 no-repeat;
    background-size: 100% 100%;
}
.page_left,.page_right{
    width: calc(50% - 20px);
    height: 100%;
    float: left;
}
.page_left{
    display: block;
    background: url("../images/bk.png") 0 0 no-repeat;
    padding: 2px;
    background-size: 100% 100%;
}
.m-title{
     line-height: 80px;
    color: #fff;
    font-size: 24px;
    padding: 0 20px;
    border-bottom: 1px solid rgba(23,255,254,0.2);
}
.m-title span{
    padding-left: 20px;
    border-left: 4px solid #17fffe;
}
.m-title div{
    float: right;
}
.warn_content{
    padding: 20px 35px 10px 50px;
    height: 460px;
}
.map_box{
    width: 56.6%;
}
.warn_content table{
    height: 100%;
    color: #fff;
    font-size: 20px;
}
.warn_content table td{
    text-align: left;
}
.warn_content img{
    float: left;
    width: 150px;
    margin-right: 20px;
}
.warn_content img+span{
    font-size: 24px;
}
.m-warning{
        height: 5%;
}
.warn_content div.m-content{
    width: calc(100% - 120px);
    height: 36px;
    line-height: 36px;
    float: right;
    margin-right: 20px;
    background: rgba(0,0,0,0.1);
    border: 1px solid rgba(23,255,255,0.2);
    border-radius: 4px;
    padding:0 40px;
}
.warn_content div.m-content.last{
    min-height: 105px;
    height: auto;
    text-align: left;
}
.warn_list{
    height: calc(100% - 541px);
    color: #fff;
    font-size: 20px;
}
.warn_list table.th{
    font-weight: 600;
    background-color: rgba(0,0,0,0.2);
}
.warn_list table.tb{
    border-bottom: 1px solid rgba(23,255,255,0.2);
}
.warn_list table tr{
   border-top: 1px solid rgba(23,255,255,0.2);
   line-height: 68px;
}
.warn_list table.tb tr.even{
    background-color: rgba(0,0,0,0.2);
}
.table_tb{
    height:calc(100% - 70px);
    overflow: hidden;
}
.MyText{
    font-size: 12px;
    color: #fff;
}
.page_right{
    float: right;
}
.m-volume{
    height: 66%;
    width: 100%;
    padding:2px;
    display: block;
    background: url(../images/bk.png) 0 0 no-repeat;
    background-size: 100% 100%;
}
.m-volume .m-title{
    font-size: 20px;
}
.m-num{
    display: inline-table;
    color: #ffe400;
    font-size: 46px;
    line-height: 80px;
    margin-left: 14px;
}
.m-volume-chart{
    height: 70%;
    padding:0 30px 20px 40px;
}
.m-chart-left,.m-chart-right{
    height: 100%;
    width: 50%;
    float: left;
    padding-top: 20px;
}
.m-chart-left{
    border-right: 1px solid rgba(23,255,255,0.2);
    padding-right: 30px;
}
.m-chart-right{
     padding-left: 30px;
     position: relative;
}
.m-chart-right>span{
    color: #00eeff;
    position: absolute;
    font-size: 24px;
    top: 66px;
}
.m-volume-chart ul{
    line-height: 42px;
    height: 42px;
}
.m-volume-chart li{
    float: left;
    font-size: 20px;
    color: #fff;
}
.m-volume-chart li.right{
    float: right;
}
.m-volume-chart li.right div{
    color: #ff9042;
    font-size: 42px;
    display: inline-block;
    margin:0 4px;
}
.m-volume-chart .m-chart{
    width: 100%;
    height: calc(100% - 42px);
}
.m-chart-right li{
    display: block;
    background:url("../images/num_bk.png") 0 0 no-repeat;
    float: left;
    width: 42px;
    height: 42px;
    text-align: center;
}
.m-volume-details{
   height: calc(30% - 82px);
    padding: 0 30px 0 40px;
}
.m-volume-details table{
    height: 100%;
}
.m-volume-details table td{
    border-right: 25px solid transparent; 
}
.m-volume-details table span{
    display: inline-table;
    width: 100%;
    border-radius: 10px;
    line-height: 32px;
    color: #fff;
    font-size: 18px;
}
.m-volume-details tr:first-child span{
    background: -webkit-linear-gradient(left, #1335c9 , #7423bf); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right, #1335c9 , #7423bf); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #1335c9 , #7423bf); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #1335c9 , #7423bf); /* 标准的语法 */
}
.m-volume-details tr:first-child td:first-child span{
    background:#1335c9;
}
.m-volume-details tr:last-child span{
    background: -webkit-linear-gradient(left, #7423bf , #c62ba0); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right, #7423bf , #c62ba0); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right, #7423bf , #c62ba0); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right, #7423bf , #c62ba0); /* 标准的语法 */
}
.m-volume-details tr:last-child td:first-child span{
    background:#8b13c9;
}
.m-feedback{
    height: calc(34% - 20px);
    width: 100%;
    padding: 2px;
    margin-top: 20px;
    background: url(../images/bk.png) 0 0 no-repeat;
    display: block;
    background-size: 100% 100%;
}
.feedback-img{
    height: calc(100% - 82px);
    overflow: hidden;
    padding: 20px 30px 30px 40px !important;
}
.m-feedback li{
    height: 100%;
    width: 360px;
    display: block;
    background: url(../images/bk.png) 0 0 no-repeat;
    background-size: 100% 100%;
    border-radius: 10px;
}
.m-feedback li img{
    height: 100%;
    width: 100%;
    position: relative;
    left: 50%;
    transform: translateX(-50%);
}
.m-img-notes{
    position: absolute;
    z-index: 99;
    width: 100%;
    color: #fff;
    font-size: 18px;
    line-height: 32px;
    padding: 0 20px;
}
.m-img-notes span{
    float: right;
}

.swiper-wrapper{
    -webkit-transition-timing-function: linear; 
    -moz-transition-timing-function: linear;
    -ms-transition-timing-function: linear;
    -o-transition-timing-function: linear;
    transition-timing-function: linear;
    margin: 0 auto;
}