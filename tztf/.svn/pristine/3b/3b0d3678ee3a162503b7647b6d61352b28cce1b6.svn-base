.m-paging {
  padding: 10px 20px;
  text-align: right;
}
.m-paging a,
.m-paging span {
  display: inline-block;
  vertical-align: top;
  font-size: 14px;
  line-height: 20px;
  color: #b2b2b2;
  margin: 0 5px;
}
.m-paging a {
  background: white;
  padding: 7px 10px;
  border: 1px solid #e6e6e6;
  cursor: pointer;
}
.m-paging a:hover {
  text-decoration: none;
  background-color: #c6c6c6;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#d9d9d9), to(#c6c6c6));
  background-image: -webkit-linear-gradient(top, #d9d9d9, #c6c6c6);
  background-image: -moz-linear-gradient(top, #d9d9d9, #c6c6c6);
  background-image: -o-linear-gradient(top, #d9d9d9, #c6c6c6);
  background-image: -ms-linear-gradient(top, #d9d9d9, #c6c6c6);
  background-image: linear-gradient(top, #d9d9d9, #c6c6c6);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#ffd9d9d9, endColorstr=#ffc6c6c6, GradientType=0);
  border-color: #b3b3b3;
  color: #808080;
}
.m-paging span {
  padding: 8px 11px;
}
