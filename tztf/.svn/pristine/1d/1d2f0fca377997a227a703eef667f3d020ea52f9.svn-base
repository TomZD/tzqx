}


/* 侧栏菜单 */

.slidebar {
    margin: 10px;
    font-size: 14px;
}

    .slidebar li a {
        text-decoration: none;
        color: #fff;
        font-weight: bold;
        line-height: 40px;
    }

    .slidebar .item {
        cursor: pointer;
        border-radius: 3px;
        margin-top: 10px;
        border: 1px solid #3E4042;
        background-color: #3E4042;
        -webkit-box-shadow: rgba(0, 0, 0, 0.21) 0px 0px 5px;
        -moz-box-shadow: rgba(0, 0, 0, 0.21) 0px 0px 5px;
        box-shadow: rgba(0, 0, 0, 0.21) 0px 0px 5px;
    }

    .slidebar .img1 {
        margin-left: 10px;
        margin-right: 10px;
    }

    .slidebar .item i {
        float: right;
        width: 16px;
        height: 16px;
        margin-right: 10px;
        margin-top: 14px;
        background: url("/images/manage/get.png") center center;
        transition: all 0.3s;
    }

    .slidebar .checklist {
        height: 0;
        overflow: hidden;
    }

    .slidebar .checklists {
        height: 0;
        overflow: hidden;
    }

    .slidebar .open .item {
        /*background: #448bcc;
    color: white;*/
    }

        .slidebar .open .item a {
            color: white;
        }

        .slidebar .open .item i {
            background-image: url("/images/manage/get1.png");
        }

    .slidebar .open .checklist {
        height: auto;
    }

    .slidebar .checked .checklists {
        height: auto;
    }

