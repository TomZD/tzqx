    border: 2px solid #bac0c6\9;
    /*IE8*/
    *border: 2px solid #bac0c6;
    /*IE7*/
}


.tools .btns li a span {
    text-indent: 1px;
    color: #333;
    text-decoration: none;
    line-height: 25px;
}

.tools .btns li a i {
    background: url(/images/toolbar_btn.png) no-repeat;
    display: inline-block;
    float: left;
    padding: 5px;
    width: 10px;
    height: 10px;
}

.tools .btns li a .toolbar_btn01 {
    background-position: 0px 2px;
}

.tools .btns li a .toolbar_btn02 {
    background-position: 0px -17px;
}

.tools .btns li a .toolbar_btn03 {
    background-position: 0px -40px;
}

.tools .btns li a .toolbar_btn04 {
    background-position: 0px -65px;
}

.tools .btns li a .toolbar_btn05 {
    background-position: 0px -92px;
}

.tools .btns li a .toolbar_btn06 {
    background-position: 0px -118px;
}

.tools .btns li a .toolbar_btn07 {
    background-position: 0px -137px;
}

.tools .btns li a:hover .toolbar_btn01 {
    background-position: -18px 2px;
}

.tools .btns li a:hover .toolbar_btn02 {
    background-position: -18px -17px;
}

.tools .btns li a:hover .toolbar_btn03 {
    background-position: -18px -40px;
}

.tools .btns li a:hover .toolbar_btn04 {
    background-position: -18px -65px;
}

.tools .btns li a:hover .toolbar_btn05 {
    background-position: -18px -92px;
}

.tools .btns li a:hover .toolbar_btn06 {
    background-position: -16px -116px;
}

.tools .btns li a:hover .toolbar_btn07 {
    background-position: -18px -137px;
}


.tools .btns li a:hover span {
