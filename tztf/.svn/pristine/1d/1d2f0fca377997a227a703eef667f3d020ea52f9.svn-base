    -moz-box-shadow: rgba(6, 0, 1, 0.5) 0px 2px 9px;
    box-shadow: rgba(6, 0, 1, 0.5) 0px 2px 9px;
}

.g-left {
    float: left;
    width: 200px;
    margin-left: 0px;
    overflow: auto;
    height: 0px;
    background-color: rgb(62,64,66);
    background: url(../images/g-left-bg.png);
    -webkit-box-shadow: 1px 0px 0px #cccccc inset, 1px 5px 12px rgba(6, 0, 1, 0.5);
    -moz-box-shadow: 1px 0px 0px #cccccc inset, 1px 5px 12px rgba(6, 0, 1, 0.5);
    box-shadow: 1px 0px 0px #cccccc inset, 1px 5px 12px rgba(6, 0, 1, 0.5);
    -webkit-transition: margin-left 0.3s;
    -moz-transition: margin-left 0.3s;
    transition: margin-left 0.3s;
    border-right: 2px solid #bac0c6\9;
    /*IE8*/
    *border-right: 2px solid #bac0c6;
    /*IE7*/
}

.g-left-m {
    margin-left: -200px;
}

.g-main {
    margin-left: 200px;
    overflow: auto;
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
    transition: all 0.3s;
}

.g-main-m {
    margin-left: 0px;
}

.g-bottom {
    width: 100%;
    height: 20px;
    line-height: 20px;
    background-color: #e7e7e7;
    text-align: center;
    color: #959595;
}

.g-right {
    width: 300px;
    background: #f5f5f5;
    float: right;
    overflow: auto;
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
    transition: all 0.3s;
}

    .g-right .top,
    .g-right .bottom {
        margin: 10px;
        padding: 10px;
        border-radius: 10px;
        box-shadow: 0px 0px 2px 2px #ccc;
        background: #fff;
        line-height: 20px;
        font-size: 14px;
        overflow: hidden;
    }

    .g-right .title {
        padding: 5px;
        width: 80px;
        margin-left: -10px;
        border-radius: 0 10px 10px 0;
        text-align: center;
        background: #679FD2;
        color: #fff;
        font-weight: bold;
        font-size: 16px;
    }

    .g-right .text {
        line-height: 25px;
        text-align: left;
        text-indent: 2em;
        margin: 5px;
        height: 100px;
        overflow: hidden;
    }
    .slidebar li.checklabel.checked {
    /*background: #A2A29E;*/
    color: #0087FF;
}

/* logo */

.m-logo {
    float: left;
    height: 100%;
}

    .m-logo a {
