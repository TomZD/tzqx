    }


/* 导航 */

.m-nav {
    margin-left: 360px;
    height: 100%;
    font-size: 15px;
}

    .m-nav table {
        width: auto;
        height: 100%;
        float: right;
    }

        .m-nav table th {
            padding: 0 10px;
        }

    .m-nav th a {
        text-decoration: none;
        color: #fff;
        text-shadow: 1px 1px 3px #000;
    }

    .m-nav .active {
        border-radius: 10px;
        -webkit-box-shadow: 0px 0px 12px rgba(0, 0, 0, 0.22) inset, 1px 0px 2px rgba(255, 255, 255, 0.19);
        -moz-box-shadow: 0px 0px 12px rgba(0, 0, 0, 0.22) inset, 1px 0px 2px rgba(255, 255, 255, 0.19);
        box-shadow: 0px 0px 12px rgba(0, 0, 0, 0.22) inset, 1px 0px 2px rgba(255, 255, 255, 0.19);
    }


