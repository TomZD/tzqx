.more-tool {
    display: none;
    z-index: 1;
    position: absolute;
    top: 100%;
    right: 0;
    width: 130px;
    background-color: #fff;
    -webkit-box-shadow: rgba(6, 0, 1, 0.5) 0px 2px 9px;
    -moz-box-shadow: rgba(6, 0, 1, 0.5) 0px 2px 9px;
